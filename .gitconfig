[user]
  name = Kairi Kozuma
  email = kairikozuma@gmail.com

[alias]
  b = branch
  ba = branch -a
  com = commit
  ch = checkout
  d = diff
  dc = diff --cached
  g = !git gui &
  lg = log --graph
  lgo = log --graph --pretty=oneline --abbrev-commit
  k = !gitk &
  ka = !gitk --all &
  st = status
[color]
  ui = auto
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = green
  changed = yellow
  untracked = red
